<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.navigation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Navigation">
        <activity android:name=".forgot_pass"></activity>
        <activity
            android:name=".dashboard"
            android:label="C h h o k a b" />
        <activity
            android:name=".prayer"
            android:label="prayer"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".prostration"
            android:label="Prostration"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".offering"
            android:label="Offering"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ritual"
            android:label="Ritual"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Navigation.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>